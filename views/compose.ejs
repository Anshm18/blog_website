<%- include('partials/header') %>

    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <h3>Title</h3>
            <input class="form-control" aria-describedby="sizing-addon2" type="text" name="postTitle">
            <h3>Post</h3>
            <textarea class="form-control" cols="30" rows="5" name="postBody"></textarea>
